<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<title>Busy Bee</title>
	  <%= favicon_link_tag "/images/favicon.ico" %>
	  <%= stylesheet_link_tag "templatemo_style.css", :media => "all" %>
	  <%= csrf_meta_tag %>
	  <%= javascript_include_tag "jquery.js", "rails.js","application.js","coda-slider.1.1.1.js","coda-slider.1.1.1.pack.js","jquery.form.js","jquery.validate.min.js","jquery-1.2.1.pack.js","jquery-easing.1.2.js","jquery-easing.1.2.pack.js","jquery-easing-compatibility.1.2.js","jquery-easing-compatibility.1.2.pack.js","logging.js"%>
	    <!-- 
	    The CSS. You can of course have this in an external .css file if you like.
	    Please note that not all these styles may be necessary for your use of Coda-Slider, so feel free to take out what you don't need.
	    -->
	    
	    <!-- Initialize each slider on the page. Each slider must have a unique id -->
	    <script type="text/javascript">
		    jQuery(window).bind("load", function() {
		    jQuery("div#slider1").codaSlider()
		    // jQuery("div#slider2").codaSlider()
		    // etc, etc. Beware of cross-linking difficulties if using multiple sliders on one page.
		    });
	    </script>
</head>
<body>

<div id="templatemo_site_title_bar_wrapper">
	<div id="templatemo_site_title_bar">
    	<div id="site_title">
            <h1><a href="http://www.busybeetech.com" target="_parent">
                <%= image_tag ("/images/rr.png"),:size => "100x50"%>
            </a></h1>
        </div>
        <div id="fb"><a href="#"></a></div>
        <div id="twitter"><a href="#"></a></div>
      
	</div> <!-- end of templatemo_site_title_bar -->
</div> <!-- end of templatemo_site_title_bar_wrapper -->
<div id="templatemo_content_wrapper">
  <div id="templatemo_content">
	<div class="slider-wrap">
		<div id="stripNav0" class="stripNav" style="width: 650px;">
			<ul>
				<li class="tab1" style="">
					<a  href="/">Home</a>
				</li>
				<li class="tab2" style="">
					<a  href="/home/show">Services</a>
				</li>
				<li class="tab3" style="">
					<a  href="/home/tech">Technology</a>
				</li>
				<li class="tab4" style="">
					<a  href="/home/about">About Us</a>
				</li>
				<li class="tab4" style="">
					<a  href="/home/contact">Contact Us</a>
				</li>
			</ul>
		</div>
		<div id="slider" class="stripViewer">
			<div class="panelContainer">
				<div class="panel" title="Home">
					<div class="wrapper">
						<%= yield :layout%>
					</div>
				</div>
			</div>
		</div>
	</div>
  </div> 
</div> <!-- end of templatemo_content_wrapper -->

<div id="templatemo_footer_wrapper">

	<div id="templatemo_footer">

	    Copyright Â© 2048 <a href="#">Busy Bee</a> | 
        Designed by <a href="#" target="_parent">Busy Bee</a> 
	</div> <!-- end of templatemo_footer -->
</div> <!-- end of templatemo_footer_wrapper -->


<script src='http://ajax.googleapis.com/ajax/libs/jquery/1.3.1/jquery.min.js'></script>
<script type='text/javascript' src='js/logging.js'></script>
</body>
</html>	
<script>
$(document).ready(function(){
	$(function() {
            $(this).bind("contextmenu", function(e) {
                e.preventDefault();
            });
        }); 
        });
</script>
